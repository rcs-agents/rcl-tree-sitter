{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rcl",
      "camelcase": "Rcl",
      "title": "Rich Communication Language",
      "scope": "source.rcl",
      "file-types": [
        "rcl"
      ],
      "injection-regex": "^rcl$",
      "class-name": "TreeSitterRcl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Rcl grammar for tree-sitter",
    "authors": [
      {
        "name": "Saulo Vallory",
        "email": "saulo@tokilabs.io",
        "url": "https://tokilabs.io/"
      }
    ],
    "links": {
      "repository": "https://github.com/rcs-agents/rcl-tree-sitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}